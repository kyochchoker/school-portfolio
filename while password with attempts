attempts = 0 
count = 4

print("You have 3 attempts to sign in.")

while attempts < 3:
    attempts += 1
    count -= 1

    password1 = input(f"You only have  {count} attempt/s left: ")
        

    if password1 == "bryanlouis06":
        print("ACCESS GRANTED!")
        break
    
    else:
        print("ACESS DENIED!")
